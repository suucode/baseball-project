<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.bb.domain.positions.PositionsDao">

	<insert id="insert">
		INSERT INTO positions(positions)
		VALUES(#{positions})
	</insert>

	<select id="findAll" resultType="site.metacoding.bb.domain.positions.Positions">
		SELECT positions FROM positions
	</select>

	<update id="update">
		
	</update>

	<delete id="deleteById">
		DELETE FROM positions WHERE id = #{id}
	</delete>

</mapper>